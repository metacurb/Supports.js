{
  "name": "supports.js",
  "version": "0.5.0",
  "description": "A fast, lightweight and dependency-free feature-detection library",
  "main": "supports.js",
  "homepage": "https://beauagst.github.io/Supports.js/",
  "repository": "github:BeauAgst/Supports.js",
  "bugs": "https://github.com/BeauAgst/Supports.js/issues",
  "scripts": {
    "lint": "eslint index.js --cache --ignore-pattern .gitignore",
    "build:min": "npx babel supports.js --out-file supports.min.js",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "build:min"
  ],
  "keywords": [
    "browser",
    "compatibility",
    "detect",
    "detection",
    "dom-api",
    "fast",
    "feature",
    "feature detection",
    "support",
    "supported",
    "zero-dependency"
  ],
  "author": "Beau August",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "eslint": "^6.8.0",
    "pre-commit": "^1.2.2"
  }
}
